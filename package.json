{
	"name": "vite-react-chakra-template",
	"version": "1.0.0",
	"author": "√çcaro C. Capobianco",
	"license": "MIT",
	"scripts": {
		"dev": "vite",
		"build": "tsc && vite build",
		"start": "tsc && yarn dev",
		"es5": "tsc -p tsconfig.json --target es6 --outDir es6 --module es6",
		"es6": "tsc -p tsconfig.json --target es5 --outDir es5 --module commonjs",
		"tsc": "concurrently \"npm run es5\" \"npm run es6\"",
		"watch": "concurrently \"npm run es5 --watch\" \"npm run es6 --watch\"",
		"storybook": "start-storybook -p 6006",
		"build-storybook": "build-storybook"
	},
	"dependencies": {
		"@chakra-ui/icons": "^1.0.5",
		"@chakra-ui/react": "^1.3.3",
		"@emotion/react": "^11.1.5",
		"@emotion/styled": "^11.1.5",
		"concurrently": "^6.0.0",
		"framer-motion": "^3.7.0",
		"react": "^17.0.0",
		"react-dom": "^17.0.0",
		"react-icons": "^4.2.0"
	},
	"devDependencies": {
		"@types/node": "^14.14.31",
		"@types/react": "^17.0.3",
		"@types/react-dom": "^17.0.0",
		"@vitejs/plugin-react-refresh": "^1.1.0",
		"concurrently": "^6.0.2",
		"typescript": "^4.2.4",
		"vite": "^2.0.1",
		"vite-aliases": "^0.5.0",
		"vite-preset-react": "^1.1.3",
		"@storybook/cli": "^6.2.8",
		"@storybook/react": "^6.2.8",
		"sb": "^6.2.8"
	}
}
